{
    "name": "igaster/laravel_cities",
	"description": "Find any country/city. Deploy geonames.org database localy. Find lang/lat etc. Optimized DB tree structure",
    "keywords": ["laravel-5.x", "geonames.org"],
    "license": "MIT",
    "homepage": "https://github.com/igaster/laravel_cities.git",
    "authors": [
        {
            "name": "Giannis Gasteratos",
            "email": "igasteratos@gmail.com"
        }
    ],	
    "require": {
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "^6.0",
        "symfony/console": "^3.2",
        "symfony/var-dumper": "^3.2",
        "monolog/monolog": "^1.22",
        "vlucas/phpdotenv": "^2.4"
    },     
    "autoload": {
        "psr-4" : {
            "Igaster\\LaravelCities\\"          : "src/",
            "Igaster\\LaravelCities\\Tests\\"   : "tests/"            
        }
    },
    "minimum-stability": "stable"
}
